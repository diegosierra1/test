<?xml version='1.0' encoding='utf-8'?>
<widget id="com.indexdream.datacity" version="1.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>DataCity</name>
    <description>
       Data City Bogota.
    </description>
    <author email="diegosierra@agendamia.com" href="http://hoturcol.com">
        Hoturcol
    </author>
    <content src="/index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon density="ldpi" height="36" platform="android" src="icon/iconoDataCity36.png" width="36" />
    <icon density="mdpi" height="48" platform="android" src="icon/iconoDataCity48.png" width="48" />
    <icon density="hdpi" height="72" platform="android" src="icon/iconoDataCity72.png" width="72" />
    <icon density="xhdpi" height="96" platform="android" src="icon/iconoDataCity96.png" width="96" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
